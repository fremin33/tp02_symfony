{% extends "base.html.twig" %} {% block body %}
<h1>Categories</h1>
<table>
    <tr>
        <th>id</th>
        <th>nom</th>
    </tr>
    {% for category in categories %}
    <tr>
        <td>{{category.id}}</td>
        <td>{{category.nom}}</td>
    </tr>
    {% endfor %}
</table>
{% endblock body %}